  },
  "report": [
    "browser",
    "json"
  ],
  "engine": "puppeteer",
  "engineOptions": {
    "args": [
      "--no-sandbox",
      "--disable-setuid-sandbox"
    ],
    "headless": true,
    "ignoreHTTPSErrors": true
