{
  "name": "diazotheme.aon2026",
  "description": "Plone Theme for AON 2026",
  "version": "1.0.0a1",
  "files": [
    "/scss",
    "/src/diazotheme/aon2026/theme/tinymce/*.css"
  ],
  "licence": "GPL-3.0-or-later",
  "browserslist": [
    "> 0.5%",
    "last 2 major versions",
    "not dead",
    "Chrome >= 60",
    "Firefox >= 60",
    "Firefox ESR",
    "iOS >= 12",
    "Safari >= 12",
    "not Explorer <= 11"
  ],
  "devDependencies": {
    "@axe-core/puppeteer": "^4.11.1",
    "@babel/core": "^7.28.3",
    "@babel/preset-env": "^7.28.3",
    "@eslint/js": "^9.37.0",
    "assets-webpack-plugin": "^7.1.1",
    "autoprefixer": "^10.4.21",
    "axe-html-reporter": "^2.2.11",
    "babel-loader": "^10.0.0",
    "backstopjs": "^6.3.25",
    "clean-css-cli": "^5.6.3",
    "eslint": "^9.34.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-n": "^17.21.3",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-webpack-plugin": "^5.0.2",
    "js-yaml": "^4.1.0",
    "nodemon": "^3.1.10",
    "npm-run-all2": "^8.0.4",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "pre-commit": "^1.2.2",
    "puppeteer": "^24.37.2",
    "sass": "1.91.0",
    "stylelint": "^16.23.1",
    "stylelint-config-twbs-bootstrap": "^16.1.0",
    "tinymce": "^7.9.1",
    "webpack": "^5.101.3",
    "webpack-cli": "^6.0.1",
    "webpack-merge": "^6.0.1"
  },
  "scripts": {
    "\n========== Watch Scripts ==========": "",
    "watch": "nodemon --watch src/diazotheme/aon2026/theme/scss --watch scss --ext scss --exec \"npm run css-main\"",
    "js-watch": "NODE_ENV=development webpack --watch --config webpack/webpack.dev.js ",
    "\n========== Build Scripts ==========": "",
    "build": "npm-run-all css-compile-main css-prefix-main css-minify-main copy-tinymce-ui-css css-compile-toolbar css-minify-toolbar js-build js-prod-build",
    "\n========== CSS Scripts ==========": "",
    "css-main": "npm-run-all css-compile-main css-prefix-main css-minify-main",
    "css-compile-main": "sass --load-path=node_modules --style expanded --source-map --embed-sources --no-error-css src/diazotheme/aon2026/theme/scss/aon2026.scss:src/diazotheme/aon2026/theme/css/theme.css",
    "css-prefix-main": "postcss --config postcss.config.js --replace \"src/diazotheme/aon2026/theme/css/*.css\" \"!src/diazotheme/aon2026/theme/css/*.min.css\"",
    "css-minify-main": "cleancss -O1 --format breakWith=lf --with-rebase --source-map --source-map-inline-sources --output src/diazotheme/aon2026/theme/css/theme.min.css src/diazotheme/aon2026/theme/css/theme.css",
    "css-compile-toolbar": "sass --load-path=node_modules --style expanded --source-map --embed-sources --no-error-css scss/barceloneta-toolbar.scss:src/diazotheme/aon2026/theme/css/barceloneta-toolbar.css",
    "css-minify-toolbar": "cleancss -O1 --format breakWith=lf --with-rebase --source-map --source-map-inline-sources --output src/diazotheme/aon2026/theme/css/barceloneta-toolbar.min.css src/diazotheme/aon2026/theme/css/barceloneta-toolbar.css",
    "css-lint": "stylelint \"scss/**/*.scss\" \"src/diazotheme/aon2026/theme/scss/**/*.scss\" --cache --cache-location .cache/.stylelintcache",
    "copy-tinymce-ui-css": "cp \"node_modules/tinymce/skins/ui/oxide/content.css\" \"src/diazotheme/aon2026/theme/tinymce/tinymce-ui-content.css\"",
    "\n========== Javascript Scripts ==========": "",
    "js-build": "npm-run-all js-dev-build js-prod-build",
    "js-dev-build": "NODE_ENV=development webpack --config webpack/webpack.dev.js ",
    "js-prod-build": "NODE_ENV=production webpack --config webpack/webpack.prod.js ",
    "\n========== BackstopJS initial run ==========": "",
    "backstop:init": "pnpm backstop:generate && backstop init",
    "\n========== Generate Configuration for BackstopJS ==========": "",
    "\n========== local v local, prod v prod, local v prod, prod v local": "",
    "backstop:generate": "node scripts/backstop-config.js > backstop.json",
    "backstop:generate:prod": "node scripts/backstop-config.js --prod > backstop.json",
    "backstop:generate:cross": "node scripts/backstop-config.js --ref-prod > backstop.json",
    "backstop:generate:prod-cross": "node scripts/backstop-config.js --prod --ref-local > backstop.json",
    "\n========== Generate reference images for BackstopJS ==========": "",
    "backstop:reference": "pnpm backstop:generate && backstop reference",
    "backstop:reference:local": "pnpm backstop:generate && backstop reference",
    "backstop:reference:prod": "pnpm backstop:generate:prod && backstop reference",
    "\n========== Run BackstopJS tests ==========": "",
    "backstop:test": "pnpm backstop:generate && backstop test",
    "backstop:test:prod": "pnpm backstop:generate:prod && backstop test",
    "backstop:test:cross": "pnpm backstop:generate:cross && backstop test",
    "backstop:test:prod-cross": "pnpm backstop:generate:prod-cross && backstop test",
    "\n========== Approve, report and remote BackstopJS scripts ==========": "",
    "\n========== Run remote after backstop.json is generated ==========": "",
    "backstop:approve": "backstop approve",
    "backstop:report": "xdg-open backstop_data/html_report/index.html",
    "backstop:remote": "backstop remote",
    "\n========== Generate configuration for Showcase BackstopJS setup ==========": "",
    "\n========== Showcase setup is for stakeholder consumption on dev server ==========": "",
    "showcase:copy-references": "node scripts/copy-showcase-references.js",
    "showcase:copy-references:dry-run": "node scripts/copy-showcase-references.js --dry-run",
    "showcase:generate": "node scripts/backstop-config.js --showcase > backstop.json",
    "showcase:generate:prod": "node scripts/backstop-config.js --showcase --prod > backstop.json",
    "showcase:generate:cross": "node scripts/backstop-config.js --showcase --ref-prod > backstop.json",
    "showcase:generate:prod-cross": "node scripts/backstop-config.js --showcase --prod --ref-local > backstop.json",
    "showcase:reference": "pnpm showcase:generate && backstop reference",
    "showcase:reference:prod": "pnpm showcase:generate:prod && backstop reference",
    "showcase:test": "pnpm showcase:generate && backstop test",
    "showcase:test:prod": "pnpm showcase:generate:prod && backstop test",
    "showcase:test:cross": "pnpm showcase:generate:cross && backstop test",
    "showcase:test:prod-cross": "pnpm showcase:generate:prod-cross && backstop test",
    "\n========== Copy results to Plone dev repo ==========": "",
    "\n========== or Run test and then copy results to Plone dev repo ==========": "",
    "showcase:process": "node scripts/process-showcase-results.js",
    "showcase:test:process": "pnpm showcase:test && pnpm showcase:process",
    "showcase:test:prod:process": "pnpm showcase:test:prod && pnpm showcase:process",
    "showcase:test:cross:process": "pnpm showcase:test:cross && pnpm showcase:process",
    "showcase:test:prod-cross:process": "pnpm showcase:test:prod-cross && pnpm showcase:process",
    "\n========== Open BackstopJS Showcase report ==========": "",
    "showcase:report": "xdg-open backstop_data/showcase_current/html_report/index.html",
    "\n========== Link Styles Testing ==========": "",
    "\n========== Test link pseudo-classes (default, hover, focus) across all link areas ==========": "",
    "link-styles:generate": "node scripts/backstop-config.js --link-styles > backstop.json",
    "link-styles:reference": "pnpm link-styles:generate && backstop reference",
    "link-styles:test": "pnpm link-styles:generate && backstop test",
    "link-styles:approve": "backstop approve",
    "link-styles:report": "xdg-open backstop_data/link_styles_html_report/index.html",
    "\n========== Link style testing does not make sense on prod until we are on Plone 6": "",
    "\n========== due to html changes.": "",
    "\nlink-styles:generate:prod": "node scripts/backstop-config.js --link-styles --prod > backstop.json",
    "\nlink-styles:generate:cross": "node scripts/backstop-config.js --link-styles --ref-prod > backstop.json",
    "\nlink-styles:generate:prod-cross": "node scripts/backstop-config.js --link-styles --prod --ref-local > backstop.json",
    "\nlink-styles:reference:prod": "pnpm link-styles:generate:prod && backstop reference",
    "\nlink-styles:test:prod": "pnpm link-styles:generate:prod && backstop test",
    "\nlink-styles:test:cross": "pnpm link-styles:generate:cross && backstop test",
    "\nlink-styles:test:prod-cross": "pnpm link-styles:generate:prod-cross && backstop test",
    "\n========== Listing View Testing ==========": "",
    "\n========== Test listing_view layout across site sections ==========": "",
    "listing-view:generate": "node scripts/backstop-config.js --listing-view > backstop.json",
    "listing-view:reference": "pnpm listing-view:generate && backstop reference",
    "listing-view:test": "pnpm listing-view:generate && backstop test",
    "listing-view:approve": "backstop approve",
    "listing-view:report": "xdg-open backstop_data/listing_view_html_report/index.html",
    "\n========== Listing view production testing ==========": "",
    "listing-view:generate:prod": "node scripts/backstop-config.js --listing-view --prod > backstop.json",
    "listing-view:generate:cross": "node scripts/backstop-config.js --listing-view --ref-prod > backstop.json",
    "listing-view:generate:prod-cross": "node scripts/backstop-config.js --listing-view --prod --ref-local > backstop.json",
    "listing-view:reference:prod": "pnpm listing-view:generate:prod && backstop reference",
    "listing-view:test:prod": "pnpm listing-view:generate:prod && backstop test",
    "listing-view:test:cross": "pnpm listing-view:generate:cross && backstop test",
    "listing-view:test:prod-cross": "pnpm listing-view:generate:prod-cross && backstop test",
    "\n========== Quick Testing (lg viewport only) ==========": "",
    "\n========== Fast testing with single viewport for rapid iteration ==========": "",
    "quick:generate": "node scripts/backstop-config.js --quick > backstop.json",
    "quick:generate:prod": "node scripts/backstop-config.js --quick --prod > backstop.json",
    "quick:generate:cross": "node scripts/backstop-config.js --quick --ref-prod > backstop.json",
    "quick:generate:prod-cross": "node scripts/backstop-config.js --quick --prod --ref-local > backstop.json",
    "quick:reference": "pnpm quick:generate && backstop reference",
    "quick:reference:prod": "pnpm quick:generate:prod && backstop reference",
    "quick:test": "pnpm quick:generate && backstop test",
    "quick:test:prod": "pnpm quick:generate:prod && backstop test",
    "quick:test:cross": "pnpm quick:generate:cross && backstop test",
    "quick:test:prod-cross": "pnpm quick:generate:prod-cross && backstop test",
    "quick:approve": "backstop approve",
    "quick:report": "xdg-open backstop_data/quick_html_report/index.html",
    "\n========== Accessibility Testing (axe-core) ==========": "",
    "\n========== Automated WCAG audits using axe-core on BackstopJS scenarios ==========": "",
    "a11y:test": "node scripts/a11y-test.js",
    "a11y:test:prod": "node scripts/a11y-test.js --prod",
    "a11y:test:quick": "node scripts/a11y-test.js --feature=showcase",
    "a11y:test:quick:prod": "node scripts/a11y-test.js --prod --feature=showcase",
    "a11y:report": "xdg-open backstop_data/a11y_results/latest.html",
    "\n========== Hover Contrast Testing (WCAG SC 1.4.3) ==========": "",
    "\n========== Automated hover-state contrast ratio checks on BackstopJS scenarios ==========": "",
    "hover:test": "node scripts/hover-contrast-test.js",
    "hover:test:prod": "node scripts/hover-contrast-test.js --prod"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.7.2",
    "bootstrap": "5.3.8"
  },
  "packageManager": "pnpm@10.13.1+sha512.37ebf1a5c7a30d5fabe0c5df44ee8da4c965ca0c5af3dbab28c3a1681b70a256218d05c81c9c0dcf767ef6b8551eb5b960042b9ed4300c59242336377e01cfad"
}
